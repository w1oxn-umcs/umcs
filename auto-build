local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/w1oxn-umcs/umcs/refs/heads/main/rayfield'))()

local Window = Rayfield:CreateWindow({
	Name = "MirrlandBot - Auto-Build",
	Icon = 0,
	LoadingTitle = "MirrlandBot - Auto-Build",
	LoadingSubtitle = "by Comrade Alexei Petrov",
	ShowText = "Script",
	Theme = "Default",

	ToggleUIKeybind = "K",

	DisableRayfieldPrompts = false,
	DisableBuildWarnings = false,

	ConfigurationSaving = {
		Enabled = true,
		FolderName = nil,
		FileName = "Big Hub"
	},

	Discord = {
		Enabled = false,
		Invite = "noinvitelink",
		RememberJoins = true
	},

	KeySystem = false,
	KeySettings = {
		Title = "Untitled",
		Subtitle = "Key System",
		Note = "No method of obtaining the key is provided",
		FileName = "Key",
		SaveKey = true,
		GrabKeyFromSite = false,
		Key = {"Hello"}
	}
})

local SaveTab = Window:CreateTab("Save & Load Build", 0)
local (alex fill ts)Section = SaveTab:CreateSection("alex fill ts")

